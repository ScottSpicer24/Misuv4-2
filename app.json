{
  "expo": {
    "name": "misu4",
    "slug": "misu4",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/MISU.png",
    "splash": {
      "image": "./src/assets/MISUv2.png",
      "resizeMode": "contain",
      "backgroundColor": "#44ABFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "package": "com.ucf.MiSuHome",
      "softwareKeyboardLayoutMode": "pan",
      "versionCode": 1,
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    }
  }
}